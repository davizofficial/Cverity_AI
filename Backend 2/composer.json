{
    "name": "cverity/backend",
    "description": "CVerity AI - Backend API for CV Analysis Platform",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "phpoffice/phpword": "^1.2",
        "smalot/pdfparser": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "CVerity\\": "lib/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
